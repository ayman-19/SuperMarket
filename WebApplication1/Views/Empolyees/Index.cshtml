@model IEnumerable<WebApplication1.Models.Empolyee>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Lname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sex)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Salary)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DnoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SupersonNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Lname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sex)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Salary)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DnoNavigation.Dnum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SupersonNavigation.Ssn)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Ssn">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Ssn">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Ssn">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
